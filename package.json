        "wcag-contrast": "^3.0.0"
    },
    "devDependencies": {
        "@types/node": "17.0.29",
        "@types/react": "18.0.8",
        "@types/react-color": "^3.0.6",
        "@types/react-dom": "18.0.0",
        "@types/styled-components": "^5.1.25",
        "@types/tinycolor2": "^1.4.3",
        "@types/wcag-contrast": "^3.0.0",
        "cypress": "^9.6.0",
        "eslint": "8.14.0",
        "eslint-config-next": "12.1.5",
